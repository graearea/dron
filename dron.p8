pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
x=64 y=64 z=3
dron = 0
tree1x=32 tree1y=0
tree2x=96 tree2y=0

trees={}

function clamp(v,mn,mx)
 return max(mn,min(v,mx))
end

function _init()
 for i=0,50 do 
  add(trees,{
   x=flr(rnd(128)),
   y=flr(rnd(128))
  })
 end
end

function _update()
	dron =(dron+1)%3
	tree1y=(tree1y+1)%128
	tree2y=(tree2y+1)%128
	
	for _,t in pairs(trees) do
	 t.y=(t.y+1)%128
	end
	
	if (btn(‚¨ÖÔ∏è)) then x=x-2 end
	if (btn(‚û°Ô∏è)) then x=x+2 end
	if (btn(‚¨ÜÔ∏è)) then y=y-2 end
	if (btn(‚¨áÔ∏è)) then y=y+2 end
 if (btn(‚ùé)) 
 then z=clamp(z+1,0,10) end
 if (btn(üÖæÔ∏è)) 
 then z=clamp(z-1,0,10) end
	
 
end

function _draw()
	cls(5)
		
	for _,t in pairs(trees) do
		drawtree(t.x,t.y)
	end

	spr(5,x,y)
	spr(dron,
		transform(x,z,64),
		transform(y,z,96))
end

function drawtree(x,y)
	for i=6,14 do
	spr(i,
		transform(x,i-6,96),
		transform(y,i-6,96))
	end
end

function transform(x,z,center)
	offset = (x-center)
	return x-z*offset/-62
end

