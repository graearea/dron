	end
end

function _draw()
	cls(2)
		
	for _,t in pairs(trees) do
		t:draw()
	end
	
	dron:draw()
end

function _init()
 reset()
end

function reset()
 for i=0,10 do 
  add(trees,tree())
 end 
end


