pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
x=64 y=64 z=3
frame = 0

function clamp(v,mn,mx)
 return max(mn,min(v,mx))
end

function _update()
	frame =(frame+1)%3
	
	if (btn(⬅️)) then x=x-1 end
	if (btn(➡️)) then x=x+1 end
	if (btn(⬆️)) then y=y-1 end
	if (btn(⬇️)) then y=y+1 end
 if (btn(❎)) 
 then z=clamp(z+1,0,10) 
 else z=clamp(z-1,0,10) end
 
end

function _draw()
	cls(3)
	spr(5,x,y)
	spr(frame,
		transform(x,z,64),
		transform(y,z,96))
end

function transform(x,z,center)
	offset = (x-center)
	return x-z*offset/-16
end

