function create_smoke(pos,locn)
 --bob="x:
 add_new_dust(pos.x,pos.y,locn.x,locn.y,10,rnd(2)+2,0.0,7)
end

function add_new_dust(_x,_y,_dx,_dy,_l,_s,_g,_f)
    add(dust, {
     x=_x,
     y=_y,
     dx=-_dx,
     dy=-_dy,
     life=_l,
     orig_life=_l,
     rad=_s,
     col=7, --set to color
     grav=0,
     fade=_f,
     draw=function(self)
      if self.life>3 then
       fillp() 
      elseif self.life%2==0 then
       fillp(0b0101101001011010.1) 
      else 
       fillp(0b1010010110100101.1) 
						end
      circfill(self.x,self.y,self.rad,6)
      circfill(self.x,self.y,self.rad-1,self.col)
      fillp()
     end,
     update=function(self)
      self.x+=self.dx*0.7
      self.y+=self.dy*0.7
      self.dy+=self.grav
        
      self.rad*=1.1
      self.life-=1
        
      self.col=self.fade            
      if self.life<0 then
        del(dust,self)
      end
     end
 })
end


